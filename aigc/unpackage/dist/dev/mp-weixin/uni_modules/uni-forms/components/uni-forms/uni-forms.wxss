/* 水平间距 */
page {
  color: #131526;
}
button::after {
  border: none;
}
button.primary {
  border-radius: 200rpx;
  background: linear-gradient(135deg, #17f25f 0%, #256af7 100%);
  color: #FFFFFF;
  border: none !important;
  font-size: 16px;
  font-weight: 400;
}
button.normal {
  border-radius: 200rpx;
  background: #FFFFFF;
  color: #A3B4CC;
  font-size: 16px;
  font-weight: 400;
}
button.big {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
}